<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningLevel>5</WarningLevel>
        <Version>0.0.0</Version>
        <Authors>Pavel Dubsky</Authors>
        <Company>Padutronics</Company>
        <Product>Padutronics Framework</Product>
        <Description>Game engine core for Windows.</Description>
        <Copyright>Copyright Â© Padutronics 2023</Copyright>
        <PackageProjectUrl>https://github.com/Padutronics/Padutronics.Gaming.Windows</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Padutronics/Padutronics.Gaming.Windows</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Padutronics.Conversion.Converters" Version="0.1.0" />
        <PackageReference Include="Padutronics.Gaming" Version="0.1.0" />
        <PackageReference Include="Padutronics.Geometry" Version="0.14.0" />
        <PackageReference Include="Padutronics.Windows.Win32.Api" Version="0.4.0" />
    </ItemGroup>
</Project>